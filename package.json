{
  "name": "@emperorschildren/react-native-map-cluster",
  "version": "0.0.1",
  "description": "Map clustering library.",
  "license": "MIT",
  "author": "yoshidan",
  "files": [
    "lib",
    "LICENSE"
  ],
  "main": "lib/index.module.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/emperorschildren/react-native-map-cluster.git"
  },
  "scripts": {
    "bundle": "NODE_OPTIONS=--openssl-legacy-provider yarn all",
    "all": "yarn clean && yarn build",
    "clean": "rimraf dist types",
    "build": "yarn build-dev && yarn build-prod && yarn build-types",
    "build-dev": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "build-types": "tsc --emitDeclarationOnly",
    "docs": "node docs.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ],
    "example/sampleProject/*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "always",
    "trailingComma": "all",
    "printWidth": 120
  },
  "devDependencies": {
    "@placemarkio/geo-viewport": "^1.0.2",
    "@types/react": "^18.2.6",
    "@types/supercluster": "^7.1.3",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^2.0.0",
    "react": "*",
    "react-native": "*",
    "react-native-maps": "*",
    "rimraf": "^6.0.1",
    "supercluster": "^8.0.1",
    "ts-loader": "^9.5.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^5.6.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "@placemarkio/geo-viewport": "^1.0.2",
    "react": "*",
    "react-native": "*",
    "react-native-maps": "*",
    "supercluster": "^8.0.1"
  },
  "packageManager": "yarn@4.5.0"
}
